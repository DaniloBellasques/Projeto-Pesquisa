{{>partials/header}}
<style>
/* Reset de estilos */
/* Resetando estilos padrão */
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: 'Inter', sans-serif;
}

/* Corpo da página */
body {
    background: #f8f9fa;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    min-height: 100vh;
    padding: 30px;
}

/* Container principal */
.container {
    width: 100%;
    max-width: 1000px;
    background: #ffffff;
    padding: 40px;
    border-radius: 12px;
    box-shadow: 0px 10px 30px rgba(0, 0, 0, 0.1);
}

/* Título da página */
h2 {
    text-align: center;
    font-size: 26px;
    font-weight: 600;
    color: #333;
    margin-bottom: 25px;
}

/* Estilo do formulário */
form {
    display: flex;
    justify-content: center;
    gap: 10px;
    margin-bottom: 20px;
}

form input {
    width: 70%;
    padding: 12px;
    font-size: 16px;
    border-radius: 6px;
    border: 1px solid #ddd;
    transition: border 0.3s;
}

form input:focus {
    border-color: #007bff;
    outline: none;
}

form button {
    padding: 12px 20px;
    font-size: 16px;
    font-weight: 600;
    border: none;
    border-radius: 6px;
    background: linear-gradient(90deg, #007bff, #0056b3);
    color: white;
    cursor: pointer;
    transition: 0.3s;
}

form button:hover {
    background: linear-gradient(90deg, #0056b3, #003f7f);
}

/* Mensagem de erro ou aviso */
p {
    text-align: center;
    font-size: 16px;
    font-weight: 500;
    color: #dc3545;
}

/* Lista de resultados */
ul {
    list-style: none;
    padding: 0;
}

/* Estilo de cada item da lista */
li {
    background: #ffffff;
    padding: 20px;
    margin-bottom: 20px;
    border-radius: 10px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    transition: 0.3s;
}

li:hover {
    transform: translateY(-3px);
    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.15);
}

/* Estilizando as seções */
h3 {
    font-size: 18px;
    font-weight: 600;
    color: #007bff;
    margin-bottom: 10px;
    border-bottom: 2px solid #007bff;
    padding-bottom: 5px;
}

/* Estruturando os dados em duas colunas */
.info-grid {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 15px;
}

.info-item {
    background: #f8f9fa;
    padding: 10px;
    border-radius: 6px;
    font-size: 14px;
    font-weight: 500;
    color: #555;
    display: flex;
    align-items: center;
    gap: 5px;
}

.info-item strong {
    color: #333;
}

/* Separador */
hr {
    border: 0;
    height: 1px;
    background: #ddd;
    margin: 20px 0;
}

/* Responsividade */
@media (max-width: 768px) {
    .container {
        padding: 30px;
    }

    form {
        flex-direction: column;
        align-items: center;
    }

    form input {
        width: 100%;
    }

    form button {
        width: 100%;
    }

    .info-grid {
        grid-template-columns: 1fr;
    }
}



</style>

